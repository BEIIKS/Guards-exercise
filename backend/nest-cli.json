{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/urls-api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/urls-api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/urls-api-gateway",
  "projects": {
    "kafka": {
      "type": "library",
      "root": "libs/kafka",
      "entryFile": "index",
      "sourceRoot": "libs/kafka/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka/tsconfig.lib.json"
      }
    },
    "messaging": {
      "type": "library",
      "root": "libs/messaging",
      "entryFile": "index",
      "sourceRoot": "libs/messaging/src",
      "compilerOptions": {
        "tsConfigPath": "libs/messaging/tsconfig.lib.json"
      }
    },
    "schemas": {
      "type": "library",
      "root": "libs/schemas",
      "entryFile": "index",
      "sourceRoot": "libs/schemas",
      "compilerOptions": {
        "tsConfigPath": "libs/schemas/tsconfig.lib.json"
      }
    },
    "url-fetcher": {
      "type": "application",
      "root": "apps/url-fetcher",
      "entryFile": "main",
      "sourceRoot": "apps/url-fetcher/src",
      "compilerOptions": {
        "tsConfigPath": "apps/url-fetcher/tsconfig.app.json"
      }
    },
    "urls-api-gateway": {
      "type": "application",
      "root": "apps/urls-api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/urls-api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/urls-api-gateway/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    }
  }
}